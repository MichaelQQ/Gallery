{
  "name": "Oysuami",
  "version": "1.0.0",
  "description": "Oyasumi gallery project",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "echo \"=> Starting\" && babel-node server/index.js",
    "bundle": "echo \"=> Bundling\" && webpack --config webpack.prod.config.js",
    "prebuild": "echo \"=> Cleaning\" && rimraf build/*",
    "build": "echo \"=> Building\" && npm run bundle",
    "pretest": "echo \"=> Linting\" && eslint ./src/**/*.js",
    "test": "echo \"=> Testing\" && babel-tape-runner src/**/*.test.js | tap-spec",
    "watch": "watch 'clear && npm run test -s' src",
    "dev": "babel-node devServer.js",
    "update": "updtr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelQQ/Oyasumi.git"
  },
  "author": "MichaelQQ",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-tape-runner": "^2.0.1",
    "deep-freeze": "0.0.1",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.1",
    "eslint-plugin-react": "^5.1.1",
    "react-addons-test-utils": "^15.0.2",
    "tap": "^5.7.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "updtr": "^0.1.14",
    "watch": "^0.18.0",
    "webpack-dev-middleware": "^1.6.1"
  },
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.15.1",
    "co": "^4.6.0",
    "express": "^4.13.4",
    "helmet": "^2.1.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-saga": "^0.10.4",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.0",
    "why-did-you-update": "0.0.8"
  },
  "engines": {
    "node": "6.0.0"
  }
}
